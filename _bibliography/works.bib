  keywords     = {nursing, information technology, digital divide, theoretical models, scoping review},
  number       = {3},
  pages        = {151--158 },
  publisher    = {Hogrefe},
  title        = {{Digitization and health inequality and equity in nursing. A scoping review}},
  doi          = {10.1024/1012-5302/a000803},
  volume       = {34},
  year         = {2021},
}


